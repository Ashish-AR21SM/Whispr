{
  "name": "Whispr_frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm install && dfx canister create Whispr_backend && dfx generate Whispr_backend && dfx deploy",
    "start": "vite --port 3000",
    "prebuild:dfx": "dfx generate",
    "build": "vite build",
    "build:full": "npm run prebuild:dfx && npm run build",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\"",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose"
  },
  "dependencies": {
    "@dfinity/agent": "^2.1.3",
    "@dfinity/candid": "^2.1.3",
    "@dfinity/identity": "^0.21.4",
    "@dfinity/principal": "^2.1.3",
    "@nfid/identitykit": "^1.0.13",
    "@types/three": "^0.179.0",
    "axios": "^1.7.1",
    "framer-motion": "^12.4.4",
    "lucide-react": "^0.541.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-router-dom": "^6.23.1",
    "three": "^0.179.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.0.1",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.38",
    "sass": "^1.63.6",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.1.3",
    "vite": "^6.1.0",
    "vite-plugin-environment": "^1.1.3"
  }
